{
  "name": "office-app-backend",
  "version": "2.0.0",
  "description": "Southland ERP Server",
  "author": "Jason Steele",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://omnibusinc@bitbucket.org/omnibusinc/office-app-backend.git"
  },
  "main": "index.js",
  "engines": {
    "node": ">=8.3.0",
    "npm": ">=5.3.0",
    "yarn": ">=0.27.5"
  },
  "scripts": {
    "start": "node index.js",
    "test": "NODE_ENV=test istanbul --include-all-sources cover _mocha -- -u bdd $(find test -name '*.js') && istanbul check-coverage && echo All tests passed 👍",
    "report-coverage": "codeclimate-test-reporter < ./coverage/lcov.info",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all"
  },
  "dependencies": {
    "bcrypt": "3.0.6",
    "bluebird": "3.5.4",
    "boom": "7.3.0",
    "bunyan": "1.8.12",
    "config": "3.1.0",
    "glob": "7.1.3",
    "good": "8.1.2",
    "good-console": "8.0.0",
    "hapi": "18.1.0",
    "hapi-auth-jwt2": "8.4.0",
    "hapi-swagger": "9.4.2",
    "http-status": "1.3.2",
    "inert": "5.1.3",
    "joi": "14.3.1",
    "jsonwebtoken": "9.0.0",
    "lodash": "4.17.11",
    "mysql2": "1.6.5",
    "newrelic": "5.6.4",
    "sequelize": "5.7.1",
    "uuid": "3.3.2",
    "vision": "5.4.4"
  },
  "devDependencies": {
    "sequelize-cli": "5.5.0"
  }
}
