  "repository": {
    "type": "git",
    "url": "https://omnibusinc@bitbucket.org/omnibusinc/office-app-backend.git"
  },
  "main": "index.js",
  "engines": {
    "node": ">=8.3.0",
    "npm": ">=5.3.0",
    "yarn": ">=0.27.5"
  },
  "scripts": {
    "start": "node index.js",
    "test": "NODE_ENV=test istanbul --include-all-sources cover _mocha -- -u bdd $(find test -name '*.js') && istanbul check-coverage && echo All tests passed 👍",
    "report-coverage": "codeclimate-test-reporter < ./coverage/lcov.info",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all"
  },
  "dependencies": {
    "bluebird": "3.5.0",
    "boom": "5.2.0",
    "bunyan": "1.8.12",
    "config": "1.26.2",
    "glob": "7.1.3",
    "good": "7.3.0",
    "good-console": "6.4.0",
    "hapi": "16.5.2",
    "hapi-swagger": "7.7.1",
    "http-status": "1.0.1",
    "inert": "4.2.1",
    "joi": "10.6.0",
    "lodash": "4.17.4",
    "mysql2": "^1.6.5",
    "newrelic": "2.2.0",
    "sequelize": "5.7.0",
    "vision": "4.1.1"
  },
  "devDependencies": {}
}
